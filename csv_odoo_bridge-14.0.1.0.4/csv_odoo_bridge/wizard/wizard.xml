<odoo>
	<data>
		<record id="import_csv_orders_form" model="ir.ui.view">
			<field name="name">import.csv.orders.form</field>
			<field name="model">import.csv.orders</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<field name='channel_id' readonly="1"/>
						</group>
						<group>
							<group>
								<field name='csv_file' required='1'/>
								<field name='csv_encoding' required='1'/>
								<field name='csv_delimiter'/>
								<field name='csv_quotechar'/>
							</group>
							<group></group>
						</group>
					</sheet>
					<separator colspan="6"/>
					<footer>
						<button
							name  ="import_now"
							string="Import Now"
							type  ="object"
							class ="oe_highlight"
						/>
						or
						<button special="cancel" string="Cancel" />
					</footer>
				</form>
			</field>
		</record>

		<record id="action_import_csv_orders" model="ir.actions.act_window">
			<field name="name">Import Orders</field>
			<field name="res_model">import.csv.orders</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>
	</data>
</odoo>
